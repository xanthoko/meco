Node thermoSensor{
    properties:
        Float temperature = '14.2'
    outPorts:
        - publisher:
            Float temperature
            Float humidity = '12.2'
}

Node ACDevice{
    properties:
        Boolean isOn = 'True'
        Float temperature
    inPorts:
        - subscriber
}

Connector temp{
    fromPort: thermoSensor.publisher
    toPort: ACDevice.subscriber
}
