Node thermoSensor{
    outPorts:
        - publisher:
            topic: asd.dsa
            message:
                Float temperature = '10'
                Float humidity
        - publisher:
            topic: asd.dasd
            message:
                Float temperature = '3'
}

Node sonarSensor{
    outPorts:
        - publisher:
            topic: asd.e
}

Node TV{
    properties:
        Boolean isOn = 'True'
    inPorts:
        - subscriber:
            topic: s.s
}

Node ACDevice{
    properties:
        Boolean isOn = 'True'
        Float temperature
    inPorts:
        - subscriber:
            topic: asd.dsa
}

