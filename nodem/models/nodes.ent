Node thermoSensor{
    outPorts:
        - publisher thPub:
            Float temperature = '10'
            Float humidity
        - publisher tPub:
            Float temperature = '3'
}

Node sonarSensor{
    outPorts:
        - publisher ssPub
}

Node TV{
    properties:
        Boolean isOn = 'True'
    inPorts:
        - subscriber tvSub
}

Node ACDevice{
    properties:
        Boolean isOn = 'True'
        Float temperature
    inPorts:
        - subscriber acSub
}

Connector temp{
    fromPort: thPub
    toPort: acSub
}
